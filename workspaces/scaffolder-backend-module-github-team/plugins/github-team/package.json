{
  "name": "@backstage-community/plugin-scaffolder-backend-module-github-team",
  "description": "A Backstage scaffolder backend module for creating GitHub teams with comprehensive member management",
  "version": "0.1.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "backstage": {
    "role": "backend-plugin-module",
    "pluginId": "scaffolder",
    "pluginPackage": "@backstage/plugin-scaffolder-backend"
  },
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "package.json": [
        "package.json"
      ]
    }
  },
  "scripts": {
    "build": "backstage-cli package build",
    "clean": "backstage-cli package clean",
    "lint:check": "backstage-cli package lint",
    "lint:fix": "backstage-cli package lint --fix",
    "postpack": "backstage-cli package postpack",
    "prepack": "backstage-cli package prepack",
    "start": "backstage-cli package start",
    "test": "backstage-cli package test --passWithNoTests --coverage",
    "tsc": "tsc",
    "prettier:check": "prettier --ignore-unknown --check .",
    "prettier:fix": "prettier --ignore-unknown --write ."
  },
  "dependencies": {
    "@backstage/backend-plugin-api": "^1.4.1",
    "@backstage/integration": "^1.16.0",
    "@backstage/plugin-scaffolder-node": "^0.10.0",
    "octokit": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@backstage/cli": "^0.33.1",
    "@backstage/plugin-scaffolder-node-test-utils": "^0.3.1"
  },
  "files": [
    "dist",
    "dist-dynamic/*.*",
    "dist-dynamic/dist/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/backstage/community-plugins",
    "directory": "workspaces/scaffolder-backend-module-github-team/plugins/github-team"
  },
  "keywords": [
    "backstage",
    "backend-plugin-module",
    "github",
    "teams",
    "scaffolder"
  ],
  "bugs": "https://github.com/backstage/community-plugins/issues",
  "maintainers": [
    "@backstage-community/maintainers-plugins"
  ],
  "author": "The Backstage Contributors"
}
